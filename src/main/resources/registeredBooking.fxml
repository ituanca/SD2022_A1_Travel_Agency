<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.effect.Glow?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.text.Font?>


<BorderPane id="mainPaneRegisteredBooking" fx:id="mainPaneRegisteredBooking" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.RegisteredBookingController">
   <center>
      <AnchorPane prefHeight="600.0" prefWidth="800.0">
         <children>
            <Label layoutX="199.0" layoutY="241.0" text="Your booking was registered!" textFill="#161369">
               <font>
                  <Font name="Comic Sans MS" size="30.0" />
               </font>
            </Label>
            <Button id="btnViewBookings" fx:id="btnViewBookings" layoutX="305.0" layoutY="307.0" mnemonicParsing="false" onAction="#viewBookings" prefHeight="53.0" prefWidth="187.0" style="-fx-background-color: DARKBLUE;" text="View your booked vacations" textFill="WHITE">
               <font>
                  <Font name="Comic Sans MS" size="12.0" />
               </font>
               <effect>
                  <Glow />
               </effect>
            </Button>
         </children>
      </AnchorPane>
   </center>
</BorderPane>
